{
  "name": "cyber-project-root",
  "version": "1.0.0",
  "private": true,
  "description": "Root package.json to proxy commands to cybersafe app",
  "scripts": {
    "dev": "cd cybersafe && npm run dev",
    "build": "cd cybersafe && npm run build",
    "lint": "cd cybersafe && npm run lint",
    "preview": "cd cybersafe && npm run preview",
    "install": "cd cybersafe && npm install"
  },
  "workspaces": [
    "cybersafe"
  ]
}